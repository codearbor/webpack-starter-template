{
  "name": "webpack-starter-template",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "main": "webpack.config.js",
  "scripts": {
    "build": "webpack --progress --mode=production",
    "server": "webpack serve --open"
  },
  "browserslist": [
    ">= 0.5%",
    "last 2 major versions",
    "not dead",
    "Chrome >= 60",
    "Firefox >= 60",
    "Firefox ESR",
    "iOS >= 12",
    "Safari >= 12",
    "not Explorer <= 11"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": []
  },
  "eslintConfig": {
    "parser": "@babel/eslint-parser",
    "extends": [
      "eslint:recommended"
    ],
    "rules": {
      "no-undef": "warn",
      "no-unused-vars": "warn"
    },
    "env": {
      "browser": true,
      "node": true,
      "jquery": true
    },
    "globals": {
      "$": "readonly",
      "jQuery": "readonly"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.5",
    "@babel/eslint-parser": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.2.0",
    "eslint": "^8.4.1",
    "eslint-webpack-plugin": "^3.1.1",
    "mini-css-extract-plugin": "^2.4.5",
    "postcss": "^8.4.5",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^7.0.2",
    "sass": "^1.45.0",
    "sass-loader": "^12.4.0",
    "terser-webpack-plugin": "^5.2.5",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0"
  }
}
